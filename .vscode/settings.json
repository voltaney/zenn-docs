{
  "workbench.colorTheme": "Dracula Theme",
  "pasteImage.insertPattern": "![${imageFileNameWithoutExt}](/${imageFilePath})",
  "pasteImage.path": "${projectRoot}/images/${currentFileNameWithoutExt}",
  "pasteImage.basePath": "${projectRoot}",
  "pasteImage.showFilePathConfirmInputBox": true,
  "pasteImage.filePathConfirmInputBoxMode": "onlyName",
  "pasteImage.defaultName": "",
  "textlint.languages": ["mdx", "markdown"],
  "textlint.autoFixOnSave": true,
  "editor.formatOnSave": true,
  "editor.formatOnPaste": false,
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#def8ff",
    "activityBar.background": "#def8ff",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#ff80e4",
    "activityBarBadge.foreground": "#15202b",
    "commandCenter.border": "#15202b99",
    "sash.hoverBorder": "#def8ff",
    "statusBar.background": "#acecfe",
    "statusBar.foreground": "#15202b",
    "statusBarItem.hoverBackground": "#7ae0fd",
    "statusBarItem.remoteBackground": "#acecfe",
    "statusBarItem.remoteForeground": "#15202b",
    "titleBar.activeBackground": "#acecfe",
    "titleBar.activeForeground": "#15202b",
    "titleBar.inactiveBackground": "#acecfe99",
    "titleBar.inactiveForeground": "#15202b99"
  },
  "peacock.color": "#acecfe"
}
